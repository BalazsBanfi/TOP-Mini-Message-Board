<%- include('../partials/head') %> <%- include('../partials/header') %>
<div class="container">
  <h1>Hi, this is my Mini messageboard</h1>
  <br /><br />
  <% const message = messages[messageId] %>
  <h2>Message from: <%= message.user %></h2>
  <p>Text: <%= message.text %></p>
  <p>Added: <%= message.added.toLocaleDateString() %></p>
  <a href="/">Back</a>
  <hr />
</div>
<%- include('../partials/footer') %>
